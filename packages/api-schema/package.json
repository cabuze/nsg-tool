    "@kubb/plugin-ts": "^3.5.6",
    "@kubb/plugin-zod": "^3.5.6",
    "prettier": "^3.4.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "zod": "^3.24.1"
